<form method="post" action="{{ path('produto_categoria_delete', {'id': produto_categorium.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ produto_categorium.id) }}">
    <button class="btn">Delete</button>
</form>
